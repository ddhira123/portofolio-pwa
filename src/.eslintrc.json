{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eqeqeq": ["warn", "allow-null"],
        "linebreak-style": ["warn", "windows"]
    },
    "ignorePatterns": ["jquery-3.5.1.min.js", "materialize.js", "materialize.min.js"]
}
